{
    "name": "Print Hello Chrome Extention",
    "version": "0.0.4",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            }
        }
    },
    "permissions": ["scripting", "tabs"],
    "host_permissions": ["http://*/*", "https://*/*"]
}